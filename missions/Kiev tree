main_parth_kie{
	slot = 3
	generic = no
	ai = yes
	potential = {
		tag = KIE
		NOT = { map_setup = map_setup_random }
	}
	has_country_shield = yes

	raise_the_head_kie = {
		icon = mission_build_up_to_force_limit
		required_missions = {  } #None
		position = 1
		provinces_to_highlight = {
      OR = {
			}
			NOT = {owned_by = ROOT}
    }
		trigger = {
			land_forcelimit = ROOT
		}
		effect = {
			add_years_of_income = 1
			add_prestige = 10
			}
		}
	}
	raise_the_taxes_kie = {
		icon = mission_alliances
		required_missions = { raise_the_head } #None
		position = 2
		trigger = {
			grown_by_development = 10 #Guilhem veut que la base tax de 3 province soient up de 2 mais j'ai pa trouvé
		}
		effect{
				production_efficiency = 0.10
			}
		}
	}
	slave's_capital_city_kie{
		icon = mission_conquer_50_development
		required_missions = { raise_the_taxes } #None
		position = 3
		provinces_to_highlight = {
      OR = {
				province = 280
			}
			NOT = {owned_by = ROOT} #c'est good ?
    }
		trigger = {
    280 = {
    higher_development_than = 30
        }
			}
		}
		effect{
			add_prestige = 15
			}
			#Guihem veut un event "Kiev la magnifique" qui donne -10% de coût de dev sur la province de Kiev + de l'attractivité commerciale.
			}
		}
	}
}
