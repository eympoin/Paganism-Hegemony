		position = 3
    }
		trigger = {
			church = 5
			}
		}
		effect{
				global_missionary_strength = 0.03
			}
		}
	}
}
	the_light_in_the_fog{
		icon = mission_rule_italy
		required_missions = { Vladimir's_legacy } #None
		position = 4
		trigger = {
			has_construction = great_project = 280 #Guilhem veut que le monument de Vladimir soit lvl 1 = ça fonctionne ?
		}
		effect{
			vassalize = VOL
			vassalize = SMO	
			vassalize = PLT
			vassalize = CHR
			add_prestige = 20
			}
		}
	}


	the_slave_needs_a_new_master{
		icon = a_place_among_the_great_powers
		required_missions = { the_light_in_the_fog } #None
		position = 5
		trigger = {
			prestige = 70
			legitimacy = 70
			total_development = 300
			treasury = 500
			dip_power = 400
		}
		effect{
			add_treasury = -500
			add_dip_power = -400
			#Event le grand banquet qui dans le lore on assassine les leaders des nations du RUS non soumises en fait des vassaux avec - de désir de liberté pendant 40 ans
			vassalize = MAZ
			vassalize = OKA
			vassalize = RYA
			vassalize = RSO
			vassalize = NZH
			create_union = NOV
			}
		}
	}
