path_droit_kie{
	slot = 4
	generic = no
	ai = yes
	potential = {
		tag = KIE
		NOT = { map_setup = map_setup_random }
	}
	has_country_shield = yes

	raise_the_troops = {
		icon = hispaniae
		required_missions = {eastern_counter_attack western_counter_attack}
		position = 2
    provinces_to_highlight = {
      OR = {
        area = yédisan_area
        area = zaporoguie_area
        area = azov_area
      }
      NOT = {owned_by = ROOT}
    }
		trigger = {
			#avoir converti 3 provinces de 2 point mili (y casse les couilles)
			}
		}
		effect = {
			event "la marche vers la mer" qui donne un bonus de 20 réduction du shock et claim sur l'ottoman = ça à la limite je peux le faire autrement que par event.
			}
		}
	}
}
fall_of_the_scourge{
  icon = mission_disrupt_portuguese_trade
  required_missions = { raise_the_troops } #None
  position = 5
  provinces_to_highlight = {
    OR = {
      area = azov_area
      area = yedisan_area
      area = zaporizhia_area
    }
    NOT = {owned_by = ROOT}
  }
  trigger = {
    azov_area = {
      type = all
      owned_by = ROOT
    }
    yedisan_area = {
      type = all
      owned_by = ROOT
    }
    zaporizhia_area = {
      type = all
      owned_by = ROOT
    }
  }
  effect{
  lower_don_area = {
    add_permanent_claim = ROOT
  }
  astrakhan_area = {
    add_permanent_claim = ROOT
  }
  lower_yik_area{
    add_permanent_claim = ROOT
    }
  saratov_yik_area{
    add_permanent_claim = ROOT
    }
  circassia_yik_area{
    add_permanent_claim = ROOT
    }
  dagestan_area{
    add_permanen_claim = ROOT
    }
  }
}
the_love_of_their_kind_is_the_hate_of_the_others{
  icon = mission_disrupt_portuguese_trade
  required_missions = { fall_of_the_scourge } #None
  position = 6
  }
  trigger = {
  um_of_owned_provinces_with = {
value = 20
in = european_steppes_region  #c'est good ?
NOT = { culture = root }
is_overseas = no
  }
  effect{
  shock_damage_received = -0.10
    }
  }
}

